{
  "name": "qnapusa",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node dist/server.js",
    "build": "./node_modules/.bin/tsc -p tsconfig.json --module commonjs --sourceMap --target ES5 && npm run routes && npm run swagger",
    "test": "export NODE_ENV=testing; mocha -r ts-node/register server/**/*.spec.ts --timeout 15000 --exit",
    "test-win": "set NODE_ENV=testing && mocha -r ts-node/register server/**/*.spec.ts --timeout 15000 --exit",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:dev": "export NODE_ENV=development; nodemon --exec ./node_modules/.bin/ts-node -- ./server/server.ts",
    "start:dev-win": "set NODE_ENV=development && nodemon --exec ./node_modules/.bin/ts-node -- ./server/server.ts",
    "deploy:production": "pm2 deploy production setup; pm2 deploy production",
    "routes": "rm -rf ./server/routes; mkdir ./server/routes; ./node_modules/.bin/tsoa routes",
    "swagger": "./node_modules/.bin/tsoa swagger"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "private": true,
  "dependencies": {
    "@joegesualdo/youtube-video-info-node": "0.0.2",
    "@joegesualdo/youtube-video-scraper-node": "0.0.3",
    "@types/express": "^4.11.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.7",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "firebase": "^4.13.1",
    "firebase-admin": "^5.12.1",
    "jsonwebtoken": "^8.2.1",
    "lcov-parse": "^1.0.0",
    "mongoose": "^4.13.14",
    "morgan": "^1.9.0",
    "npm": "^6.1.0",
    "rxjs": "^5.5.11",
    "tsoa": "^2.1.5",
    "tsoa-nc": "^2.1.8",
    "winston": "^2.4.2",
    "youtube-info": "^1.2.0",
    "youtube-node": "^1.3.2",
    "youtube.get-video-info": "^1.0.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/language-service": "^5.2.11",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^6.0.111",
    "@types/sinon": "^5.0.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "codelyzer": "^4.3.0",
    "dirty-chai": "^2.0.1",
    "gulp": "^4.0.0",
    "gulp-mocha": "^6.0.0",
    "istanbul": "^0.4.5",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jshint": "^2.9.5",
    "jslint": "^0.12.0",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^4.1.0",
    "node-mocks-http": "^1.7.0",
    "nodemon": "^1.17.5",
    "protractor": "^5.3.2",
    "sinon": "^4.5.0",
    "time-stamp": "^2.0.0",
    "ts-node": "^3.3.0",
    "tslint": "~5.7.0",
    "tslint-no-unused-expression-chai": "^0.1.3",
    "typescript": "^2.8.3"
  }
}
