{
  "swagger": {
    "outputDirectory": "./dist/swagger",
    "entryFile": "./server/server.ts",
    "host": "go.qnap.com",
    "basePath": "/api",
    "schemes": ["https"],
    "securityDefinitions": {
      "JWT": {
        "type": "apiKey",
        "name": "x-access-token",
        "in": "header"
      }
    }
  },
  "routes": {
    "entryFile": "./server/server.ts",
    "routesDir": "./server/routes",
    "outputDirectory": "./server/swagger",
    "authenticationModule": "./server/controllers/auth/middleware/authentication.ts",
    "basePath": "/api"
  }
}